// Simple VPC test for plugin destroy
cloud_vendor "aws" {
    region = "us-east-1"
    profile = "kyaw-zin"
}

declare vpc_config = {
    cidr_block: "10.0.0.0/16"
    enable_dns_hostnames: true
    enable_dns_support: true
    tags: {
        Name: "test-vpc"
        Environment: "test"
    }
}

declare test_vpc = vpc("test-vpc", vpc_config);
